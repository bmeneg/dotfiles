set folder = ~/Mail/bmeneg-heredoc/
set spoolfile = +inbox
set postponed = +drafts
#set mbox = '+bmeneg-heredoc/archive'
set record = +sent

mailboxes $spoolfile $postponed \
          +sent

set sendmail = "/usr/bin/msmtp -C /home/bmeneg/.msmtp/msmtprc -a bmeneg-heredoc"
set from = "bmeneg@heredoc.io"
set signature = "/home/bmeneg/.mutt/bmeneg-heredoc/signature.txt"
